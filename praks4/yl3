#!/bin/bash
#
#
echo "Sisestage reisjate arv" #reisijate arvu k체simine
read reisjad

echo "Sisestage kohtade arv" #kohtade arvu k체simine
read kohad

#
bussis=`expr $bussis \*$kohad` #kohade arvutamine

bkarv=`expr $bussis \*kohad` #arvutan mitu kohta on saadaval

mjaa=`expr $reisjad - $bkarv` #reisiate arv miinus kohad

echo "T채idetud busside arv: $bussis bussi; Bussi ei mahu $mjaa inimest"
#
# l천pp
